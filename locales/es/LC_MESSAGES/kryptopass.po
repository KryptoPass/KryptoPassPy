#: Python310/Lib/argparse.py:296
msgid "usage: "
msgstr "uso: "

#: Python310/Lib/argparse.py:868
msgid ".__call__() not defined"
msgstr ".__call__() no definido"

#: Python310/Lib/argparse.py:1211
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "analizador desconocido %(parser_name)r (opciones: %(choices)s)"

#: Python310/Lib/argparse.py:1271
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "argumento \"-\" con modo %r"

#: Python310/Lib/argparse.py:1280
#, python-format
msgid "can't open '%(filename)s': %(error)s"
msgstr "no se puede abrir '%(filename)s': %(error)s"

#: Python310/Lib/argparse.py:1489
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "no se pueden fusionar acciones - dos grupos se llaman %r"

#: Python310/Lib/argparse.py:1527
msgid "'required' is an invalid argument for positionals"
msgstr "'requerido' es un argumento inválido para los posicionales"

#: Python310/Lib/argparse.py:1549
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"cadena de opción inválida %(opción)r: debe comenzar con un carácter "
"%(prefix_chars)r"

#: Python310/Lib/argparse.py:1567
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= es necesario para opciones como %r"

#: Python310/Lib/argparse.py:1584
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "valor de resolución_conflicto inválido: %r"

#: Python310/Lib/argparse.py:1604
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "cadena de opción conflictiva: %s"
msgstr[1] "cadenas de opciones conflictivas: %s"

#: Python310/Lib/argparse.py:1668
msgid "mutually exclusive arguments must be optional"
msgstr "los argumentos mutuamente excluyentes deben ser opcionales"

#: Python310/Lib/argparse.py:1736
msgid "positional arguments"
msgstr "argumentos posicionales"

#: Python310/Lib/argparse.py:1737
msgid "options"
msgstr "opciones"

#: Python310/Lib/argparse.py:1752
msgid "show this help message and exit"
msgstr "mostrar este mensaje de ayuda y salir"

#: Python310/Lib/argparse.py:1783
msgid "cannot have multiple subparser arguments"
msgstr "no puede tener múltiples argumentos subparser"

#: Python310/Lib/argparse.py:1835
#: Python310/Lib/argparse.py:2350
#, python-format
msgid "unrecognized arguments: %s"
msgstr "argumentos no reconocidos: %s"

#: Python310/Lib/argparse.py:1936
#, python-format
msgid "not allowed with argument %s"
msgstr "no permitido con argumento %s"

#: Python310/Lib/argparse.py:1986
#: Python310/Lib/argparse.py:2000
#, python-format
msgid "ignored explicit argument %r"
msgstr "argumento explícito ignorado %r"

#: Python310/Lib/argparse.py:2107
#, python-format
msgid "the following arguments are required: %s"
msgstr "se requieren los siguientes argumentos: %s"

#: Python310/Lib/argparse.py:2122
#, python-format
msgid "one of the arguments %s is required"
msgstr "se requiere uno de los argumentos %s"

#: Python310/Lib/argparse.py:2165
msgid "expected one argument"
msgstr "esperaba un argumento"

#: Python310/Lib/argparse.py:2166
msgid "expected at most one argument"
msgstr "se espera como máximo un argumento"

#: Python310/Lib/argparse.py:2167
msgid "expected at least one argument"
msgstr "esperaba al menos un argumento"

#: Python310/Lib/argparse.py:2171
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "argumento %s esperado"
msgstr[1] "argumentos %s esperados"

#: Python310/Lib/argparse.py:2229
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "Opción ambigua: %(option)s podría coincidir con %(matches)s"

#: Python310/Lib/argparse.py:2293
#, python-format
msgid "unexpected option string: %s"
msgstr "cadena de opción inesperada: %s"

#: Python310/Lib/argparse.py:2490
#, python-format
msgid "%r is not callable"
msgstr "%r no se puede llamar"

#: Python310/Lib/argparse.py:2507
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "valor %(type)s inválido: %(value)r"

#: Python310/Lib/argparse.py:2518
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "elección no válida: %(value)r (elegir entre %(choices)s)"

#: Python310/Lib/argparse.py:2594
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: error: %(message)s\n"

